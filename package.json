    "babel-core": "^6.17.0",
    "babel-loader": "^9.1.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^6.7.1",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.0.1",
    "eslint-watch": "^2.1.14",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.5.0",
