    "nock": "^8.1.0",
    "npm-audit-helper": "^2.3.20",
    "npm-run-all": "^3.1.1",
    "nsp": "^2.6.2",
    "numeral": "^1.5.3",
    "open": "6.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "surge": "^0.20.4",
    "webpack": "^5.74.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-md5-hash": "^0.0.5"
